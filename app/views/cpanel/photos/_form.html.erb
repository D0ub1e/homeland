<%= form_for([:cpanel,@photo] , :html => { :enctype =>  "multipart/form-data" }) do |f| %>
  <%= render 'shared/error_messages', :target => @photo %>

  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :image %><br />
    <%= f.file_field :image %>
  </div>
  <div class="actions">
    <button type="submit" class="button submit">上传</button>
  </div>
<% end %>
